    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Temperatura" type="Integer" array="False" size=""/>
            <while expression="true">
                <output expression="&quot;Digite a Temperatura&quot;" newline="True"/>
                <input variable="temperatura"/>
                <breakpoint expression="" always="True"/>
                <if expression="temperatura &gt;= 30">
                    <then>
                        <output expression="&quot;Liga o ar&quot;" newline="True"/>
                        <output expression="&quot;Liga o ventilador&quot;" newline="True"/>
                    </then>
                    <else>
                        <if expression="temperatura &gt;= 20">
                            <then>
                                <output expression="&quot;Liga o ventilador&quot;" newline="True"/>
                                <output expression="&quot;Desliga o ar&quot;" newline="True"/>
                            </then>
                            <else>
                                <output expression="&quot;Desliga ventilador&quot;" newline="True"/>
                                <output expression="&quot;Desliga o ar&quot;" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
            </while>
        </body>
